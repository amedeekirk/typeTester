{{> header}}
<main>
    <div id="wrapper">
        <div id="word-bank">
            <span  v-for="item in wordlist" v-bind:id="item.ID">{{item.word}}</span>
        </div>
        <span><button id="timer" alt="Click to hide">{{counter}}</button></span>
        <input type="text" id="input" spellcheck="off" v-model="input" @keydown ="timerstart" @keyup.space="update" :disabled="finished == 1">
        <span><button id ="f5" @click="restart">F5</button></span>
    </div>
</main>